<div>
  <h1>Fiesta: {{event.name}}</h1>
  
  <h3>Description: {{event.description}}</h3>
  
  <ul>
    <li>Pool: {{event.options.hasPool}}</li>
    <li>BBQ: {{event.options.hasBBQ}}</li>
    <li>Children: {{event.options.hasChildren}}</li>
    <li>Private:  {{event.options.isPrivate}}</li>
    <li>Roof:  {{event.options.hasRoof}}</li>
    <li>Parking: {{event.options.hasParking}}</li>
  </ul>
  
</div>

<div>
  <h1>Lista de invitados</h1>
  <ul>
    {{#each users}}

      <li>{{username}}</li>

    {{/each }}



  </ul>





</div>

